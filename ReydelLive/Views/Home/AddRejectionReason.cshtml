
@{
    ViewBag.Title = "AddRejectionReasonMaster";
    Layout = null;
}


<div class="modal fade" id="RejectionReasonMaster" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Add Rejection Reason</h4>
            </div>
            <div class="modal-body">
                <form role="form">

                    <div class="form-group">
                        <label>Rejection Reason</label>
                        <input type="text" class="form-control" id="RejectionReasonName" placeholder="Rejection Reason">
                    </div>

                </form>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary" id="btnAdd">Submit</button>
            </div>
        </div>

    </div>
</div>



