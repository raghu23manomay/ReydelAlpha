

@{
    Layout = null;
}

<div class="modal fade" id="AddSupportFunctionMaster" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Add Support Function </h4>
            </div>
            <div class="modal-body">
                <form role="form">
                    <div class="form-group" hidden>
                        <label>Support FunctionId</label>
                        <input type="text" class="form-control" id="SupportFunction_Id" placeholder="Support Function Id">
                    </div>
                    <div class="form-group">
                        <label>Support Function Name</label>
                        <input type="text" class="form-control" id="SupportFunction_Name" placeholder="Support Function Name">
                    </div>
                    <div class="form-group">
                        <label>Support Function Code</label>
                        <input type="text" class="form-control" id="SupportFunction_Code" placeholder="Support Function Code">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary" id="btnAdd">Submit</button>
            </div>
        </div>

    </div>
</div>


