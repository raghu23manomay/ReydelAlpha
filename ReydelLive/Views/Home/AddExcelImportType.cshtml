 
@{
    ViewBag.Title = "AddExcelImportType";
    Layout = null;
} 
<div class="modal fade" id="ExcelImportsTypes" role="dialog" style="padding-top: 70px;">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Add Excel Import Types </h4>
            </div>
            <div class="modal-body">
                <form role="form"> 
                    <div class="form-group">
                        <label>Import Type  </label>
                        <input type="text" class="form-control" id="ImportTypeName" placeholder="Import Type">
                    </div> 
                    <div class="form-group">
                        <label>Import Modes</label>
                        <input type="text" class="form-control" id="ImportTypeModes" placeholder="Import Modes">
                    </div> 
                </form>
            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-primary" id="btnAdd">Save</button>
            </div>
        </div>  
    </div>
</div>



